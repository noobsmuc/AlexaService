{
    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "SkillApplicationId": "<your skill id>",
    "Log4net": [
        {
            "XPath": "/log4net/root/level",
            "Attributes": {
                "Value": "ALL"
            }
        },
        {
            "XPath": "/log4net/appender[@name='RollingFileAppender']/file",
            "Attributes": {
                "Value": "Log\\alexa_.log"
            }
        }
    ]
}
